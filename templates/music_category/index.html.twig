{% extends 'base.html.twig' %}

{% block title %}MusicCategory index{% endblock %}

{% block body %}
    <h1>MusicCategory index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Category</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for music_category in music_categories %}
            <tr>
                <td>{{ music_category.id }}</td>
                <td>{{ music_category.category }}</td>
                <td>
                    <a href="{{ path('app_music_category_show', {'id': music_category.id}) }}">show</a>
                    <a href="{{ path('app_music_category_edit', {'id': music_category.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_music_category_new') }}">Create new</a>
{% endblock %}
