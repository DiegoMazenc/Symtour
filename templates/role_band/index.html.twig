{% extends 'base.html.twig' %}

{% block title %}RoleBand index{% endblock %}

{% block body %}
    <h1>RoleBand index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Role_name</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for role_band in role_bands %}
            <tr>
                <td>{{ role_band.id }}</td>
                <td>{{ role_band.roleName }}</td>
                <td>
                    <a href="{{ path('app_role_band_show', {'id': role_band.id}) }}">show</a>
                    <a href="{{ path('app_role_band_edit', {'id': role_band.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_role_band_new') }}">Create new</a>
{% endblock %}
